#include <main.h>
#include <stdarg.h>

/**
* _printf - prints any kind of data type
* @format: contains format of arguments
*
*
* Return: characters counted
*/

int _printf(const char * format, ...)
{
	unsigned int i, count = 0;
	char *s;
	char *mv;

	va_list arg;
	va_start(format, arg);

for (mv = format; *mv != '\0'; mv++)
{
	while (*mv != '%')
	{
		mv++;
		count++;
		return(count);
	}
	mv++;
		switch (*mv):
		{
			case 'c': i = va_arg(arg, int);
					return(i);
					break;

			case 's': s = va_arg(arg, char *);
					return(s);
					break;
		}
}
va_end(arg);
}
